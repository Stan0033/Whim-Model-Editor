<Window x:Class="Whim_GEometry_Editor.Dialogs.DuplicateSequence"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Whim_GEometry_Editor.Dialogs"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        KeyDown="Window_KeyDown"
        WindowStartupLocation="CenterScreen"
        Background="LightGray"
        Title="Duplicate Sequence" Height="180" Width="400">
    <StackPanel Margin="10">
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Name of duplicated sequence:" Width="180" Margin="5"/>
            <TextBox Name="InputName" Width="100" Margin="5"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal">
            <TextBlock Text="Start from track:" Width="180" Margin="5"/>
            <TextBox Name="InputTrack" Width="100" Margin="5"/>
        </StackPanel>
        <Button Padding="10" Margin="10" Content="OK" Click="ok"/>
    </StackPanel>
</Window>

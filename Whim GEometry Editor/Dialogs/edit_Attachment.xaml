<Window x:Class="Whim_GEometry_Editor.Dialogs.edit_Attachment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Whim_GEometry_Editor.Dialogs"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        mc:Ignorable="d"
        KeyDown="Window_KeyDown"
        Background="LightGray"
        Title="Edit Attachment" Height="128" Width="360">
    <StackPanel>
        <StackPanel Orientation="Horizontal" Margin="5">
            <TextBlock Text="Model Path:" Margin="5" Name="InputName"/>
            <TextBox Width="200" Margin="5" FontSize="14"/>
            <Button Width="50" Click="set">Set</Button>
        </StackPanel>
        <Button Padding="5" Margin="10" Click="editvisiblity">Visibility</Button>
    </StackPanel>
</Window>

<Window x:Class="Whim_GEometry_Editor.Dialogs.edit_geo_vis"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Whim_GEometry_Editor.Dialogs"
        mc:Ignorable="d" Background="LightGray"
        KeyDown="Window_KeyDown"
        Title="edit_geo_vis" Height="270" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition Height="40"></RowDefinition>
        </Grid.RowDefinitions>
        <ListBox Grid.Row="0" Name="List_Vis" Background="LightGray" Foreground="Black" Height="200"/>
        <Button Grid.Row="1" Padding="10" Click="Set">Set</Button>
    </Grid>
</Window>

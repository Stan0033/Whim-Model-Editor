<Window x:Class="Whim_GEometry_Editor.Dialogs.ResizeSequence"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Whim_GEometry_Editor.Dialogs"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Background="LightGray"
        KeyDown="Window_KeyDown"
        Title="Resize Sequence" Height="140" Width="300">
    <StackPanel Margin="10">
        <TextBlock Name="Initial" Text="123232"/>
        <StackPanel Orientation="Horizontal" Margin="5">
            <TextBox Name="From" Width="100"/>
            <TextBlock   Text="-" Margin="5,0,5,0"/>
            <TextBox Name="To" Width="100"/>
        </StackPanel>
        <Button Padding="5" Content="Resize" Click="Resize" Margin="10"/>
    </StackPanel>
</Window>

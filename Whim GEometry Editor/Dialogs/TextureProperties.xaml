<Window x:Class="Whim_GEometry_Editor.Dialogs.TextureProperties"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Whim_GEometry_Editor.Dialogs"
        mc:Ignorable="d" Background="LightGray"
        ResizeMode="NoResize"
        KeyDown="Window_KeyDown"
        WindowStartupLocation="CenterScreen"
        Title="Properties" Height="120" Width="150">
    <StackPanel Margin="10">
        <CheckBox Name="Check_W" Content="Wrap Width" Margin="5" Checked="Check_WW" Unchecked="Check_WW"/>
        <CheckBox Name="Check_H" Content="Wrap Height" Margin="5" Checked="Check_WH" Unchecked="Check_WH"/>

    </StackPanel>
</Window>

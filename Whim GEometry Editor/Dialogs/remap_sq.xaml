<Window x:Class="Whim_GEometry_Editor.Dialogs.remap_sq"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Whim_GEometry_Editor.Dialogs"
        mc:Ignorable="d" Background="LightGray"
        WindowStartupLocation="CenterScreen"
        ResizeMode="NoResize"
        KeyDown="Window_KeyDown"
        Title="Remap All Sequences" Height="180" Width="380">
    <StackPanel Margin="10">
        <TextBlock Text="Starting from track 0, all sequences will have duration:"/>
        <TextBox Name="input" Width="200" Padding="5" Margin="5"/>
        <TextBlock Text="All sequences will be ordered and with equally spaced intervals"/>
        <Button Content="OK" Padding="10" Click="set" Margin="5"/>
    </StackPanel>
</Window>
